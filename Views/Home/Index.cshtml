@{
    ViewData["Title"] = "Home Page";
}
@model List<Student>;

<div class="text-center">
    <h1 class="display-4">Welcome this is basic hosting</h1>
    <img src="~/images/Block history_mobile.png" height="500px" />
    <h2>This change in a Dev Branch</h2>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<table>
    <tr>
        <td>ROlle No</td>
        <td>Name</td>
        <td>Class</td>
        <td>Age</td>
        <td>Pass</td>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Rollno</td>
            <td>@item.Name</td>
            <td>@item.Classid</td>
            <td>@item.Age</td>
            <td>@item.Pass</td>
        </tr>
    }
</table>